{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "allowedSlippage": {
            "type": "string"
        },
        "gasLimitEstimate": {
            "type": "integer"
        },
        "ttl": {
            "type": "integer"
        },
        "capitalProvider": {
            "type": "string"
        },
        "contractAddresses": {
            "type": "object",
            "patternProperties": {
                "^\\w+$": {
                    "type": "object",
                    "properties": {
                        "routerAddress": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "routerAddress"
                    ],
                    "additionalProperties": false
                }
            },
            "additionalProperties": false
        }
    },
    "additionalProperties": true,
    "required": [
        "allowedSlippage",
        "ttl",
        "contractAddresses",
        "capitalProvider"
    ]
}